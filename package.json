{
  "name": "ollama-coder-bench",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "cli": "node --loader ts-node/esm src/cli/index.ts",
    "prepare-models": "npm run cli -- prepare-models",
    "bench": "npm run cli -- run",
    "bench-max": "node --loader ts-node/esm scripts/bench-max.ts",
    "summarize": "npm run cli -- summarize results.json",
    "test": "vitest run tests/"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "@vitest/coverage-v8": "^4.0.15",
    "commander": "*",
    "eslint": "*",
    "execa": "*",
    "globals": "^16.5.0",
    "prettier": "*",
    "ts-node": "*",
    "typescript": "*",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}